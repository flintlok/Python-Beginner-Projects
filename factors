def factors():
    try:
        while True:
            n = int(input('Enter the number: '))
            if n > 0:
                break
            else:
                print('Please input a positive number')
            
        lst = []
        for i in range(1, n+1):
            if n % i == 0:
                lst.append(i)
        return lst
    
    except ValueError:
        print('Please input a valid digit')
